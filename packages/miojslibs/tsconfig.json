{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "sourceMap": true,
    "declaration": true,
    "outDir": "types",
    "strict": false,
    "noImplicitThis": false,
    "removeComments": true,
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "typeRoots": [
      "../../node_modules/@types/",
      "../../types/"
    ],
    "types": [
      "miojs",
      "miojs_internal"
    ]
  },
  "include": [
      "../../source/MIOCore",
      "../../source/MIOCorePlatform",
      "../../source/MIOData",
      "../../source/MIOFoundation",
      "../../source/MIOUI",
      "../../source/MIOWebServicePersistentStore",
      "../../source/MIORemoteNotificationServer",
      "../../source/index.webapp.ts"
  ],
  "exclude": [
      "../../source/MIOCorePlatform/WebWorkerClasses"
  ]
}
