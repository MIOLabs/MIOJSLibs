{
  "name": "miojslibs",
  "version": "0.9.6",
  "description": "These libraries are designing to develop a web or web application in the same way like if you develop a native iOS/macOS application.",
  "main": "dist/js/miojslibs.js",
  "types": "./typings/miojslibs/index.d.ts",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "dependencies": {
    "brace": "^0.11.1",
    "chart.js": "^2.7.2",
    "decimal.js": "^10.0.0"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^4.0.1",
    "copy-webpack-plugin": "^4.5.1",
    "copyfiles": "^2.0.0",
    "cross-env": "^5.1.4",
    "dts-bundle": "^0.7.3",
    "ifdef-loader": "^2.0.3",
    "npx": "^10.0.1",
    "rimraf": "^2.6.2",
    "ts-loader": "^4.1.0",
    "typescript": "^2.7.2",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.12",
    "webpack-shell-plugin": "^0.5.0"
  },
  "scripts": {
    "clean:build": "rimraf build/*",
    "clean:dist": "rimraf dist/*",
    "dev": "webpack",
    "postdev": "npm run copy:dist && npm run build:typing",
    "test": "echo \"Error: no test specified\" && exit 1",
    "preprod": "npm install && npm run clean:build && npm run clean:dist",
    "prod": "cross-env NODE_ENV=prod npm run build:libs && cross-env NODE_ENV=prod npm run build:webworkers",
    "copy:dist": "copyfiles \"build/**/*.js*\" \"build/*.js*\" dist/js --up 1",
    "postprod": "npm run copy:dist && npm run build:typing",
    "build:webworkers": "./tools/build_project/build_webworkers.sh",
    "build:libs": "webpack",
    "build:typing": "npm run build:typing_global",
    "build:typing_module": "./tools/build_project/bundle_typing/bundle_typing.sh",
    "build:typing_global": "./tools/build_project/bundle_typing/global_typing.sh",
    "check_missing_barrels": "python3 ./tools/build_project/check_exports/check_exports.py",
    "lint": "tslint -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MIOLabs/MIOJSLibs.git"
  },
  "keywords": [],
  "author": "miogds",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/MIOLabs/MIOJSLibs/issues"
  },
  "homepage": "https://github.com/MIOLabs/MIOJSLibs#readme"
}
